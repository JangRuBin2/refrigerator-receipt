# ===========================================
# Supabase
# ===========================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ===========================================
# Google Cloud Vision (OCR)
# ===========================================
GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id
# JSON 형식의 서비스 계정 키 (한 줄로)
GOOGLE_CLOUD_CREDENTIALS_JSON={"type":"service_account","project_id":"..."}

# ===========================================
# Google Gemini (AI)
# ===========================================
GOOGLE_GEMINI_API_KEY=your_gemini_api_key

# ===========================================
# YouTube Data API v3 (Recipe Search)
# ===========================================
# 무료 할당량: 하루 10,000 단위 (검색 1회 = 100단위, 약 100회/일)
# 발급: https://console.cloud.google.com/apis/library/youtube.googleapis.com
YOUTUBE_API_KEY=your_youtube_api_key

# ===========================================
# Google Custom Search API (Recipe Search)
# ===========================================
# 무료 할당량: 하루 100회 검색
# API 키: https://console.cloud.google.com/apis/library/customsearch.googleapis.com
# 검색 엔진 ID: https://programmablesearchengine.google.com/
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_SEARCH_ENGINE_ID=47b7e9fc49a7242b0

# ===========================================
# App Settings
# ===========================================
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_BASE_URL=https://mealkeeper.app

# ===========================================
# SEO & Search Console Verification
# ===========================================
# Google Search Console: https://search.google.com/search-console
GOOGLE_SITE_VERIFICATION=your_google_verification_code

# Naver Search Advisor: https://searchadvisor.naver.com/
NAVER_SITE_VERIFICATION=your_naver_verification_code

# Yandex Webmaster: https://webmaster.yandex.com/
YANDEX_VERIFICATION=your_yandex_verification_code

# ===========================================
# Apps in Toss (In-App Purchase)
# ===========================================
# 앱인토스 콘솔: https://console.apps-in-toss.toss.im/
# 개발자 문서: https://developers-apps-in-toss.toss.im/

# 앱 ID (콘솔에서 발급)
APPS_IN_TOSS_APP_ID=your_app_id

# mTLS 인증서 (base64 인코딩)
# 콘솔 > mTLS 인증서 탭에서 발급 후 base64로 인코딩
# Linux/Mac: base64 -i cert.pem | tr -d '\n'
# Windows (PowerShell): [Convert]::ToBase64String([System.IO.File]::ReadAllBytes("cert.pem"))
APPS_IN_TOSS_MTLS_CERT=your_mtls_cert_base64
APPS_IN_TOSS_MTLS_KEY=your_mtls_key_base64

# API URL (기본값 사용)
APPS_IN_TOSS_API_URL=https://api.apps-in-toss.toss.im

# 연결 끊기 콜백 Basic Auth (콘솔의 Basic Auth 헤더 필드에 입력한 값과 동일)
TOSS_DISCONNECT_AUTH=your_base64_encoded_basic_auth_token
